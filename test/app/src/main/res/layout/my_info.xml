<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <TextView
            android:text="닉네임"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/nickname" android:layout_marginStart="73dp"/>
    <TextView
            android:text="이메일"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/textView"
            android:layout_marginTop="224dp"/>
    <TextView
            android:text="나이"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="@+id/textView2"
            app:layout_constraintBaseline_toBaselineOf="@+id/age"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nickname"
            android:layout_marginStart="26dp" app:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_marginBottom="44dp" app:layout_constraintBottom_toTopOf="@+id/email"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/email"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView2" android:layout_marginStart="26dp"
            app:layout_constraintStart_toEndOf="@+id/textView2"/>
    <TextView
            android:text="age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/age" android:layout_marginTop="47dp"
            android:layout_marginStart="39dp"
            app:layout_constraintTop_toBottomOf="@+id/email" app:layout_constraintStart_toEndOf="@+id/textView3"/>
</androidx.constraintlayout.widget.ConstraintLayout>