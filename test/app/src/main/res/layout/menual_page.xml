<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <Button
            android:text="좌석"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:id="@+id/seat"
            app:layout_constraintBaseline_toBaselineOf="@+id/PC_cafe" app:layout_constraintStart_toEndOf="@+id/PC_cafe"
            android:layout_marginEnd="81dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="피시방 찾기"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:id="@+id/PC_cafe"
            android:layout_marginTop="169dp"
            app:layout_constraintEnd_toStartOf="@+id/seat" android:layout_marginBottom="170dp"
            app:layout_constraintTop_toTopOf="@+id/info" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="83dp"
            app:layout_constraintBottom_toBottomOf="@+id/pc_logout" android:layout_marginEnd="47dp"
    />
    <Button
            android:text="자주가는\n피시방"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/list"
            app:layout_constraintStart_toEndOf="@+id/info" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/info" app:layout_constraintBottom_toBottomOf="@+id/info"
            android:layout_marginEnd="81dp"/>
    <Button
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:id="@+id/logout"
            android:text="Logout"
            app:layout_constraintStart_toEndOf="@+id/pc_logout" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="62dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="81dp"/>
    <Button
            android:text="내 정보"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:id="@+id/info"
            android:layout_marginTop="230dp"
            app:layout_constraintEnd_toStartOf="@+id/list" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="83dp" android:layout_marginEnd="47dp"/>
    <Button
            android:text="PC 종료"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:id="@+id/pc_logout"
            app:layout_constraintBaseline_toBaselineOf="@+id/logout" app:layout_constraintEnd_toStartOf="@+id/logout"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="80dp" android:layout_marginEnd="50dp"/>
    <TextView
            android:text="PC Cafe : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/pc_cafe" android:layout_marginStart="83dp"/>
    <TextView
            android:text="  "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/pc_cafe"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="92dp"
            android:layout_marginStart="12dp" app:layout_constraintStart_toEndOf="@+id/textView6"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/local"
            app:layout_constraintBaseline_toBaselineOf="@+id/pc_cafe" android:layout_marginStart="12dp"
            app:layout_constraintStart_toEndOf="@+id/pc_cafe"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/a" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="91dp" android:layout_marginEnd="126dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>